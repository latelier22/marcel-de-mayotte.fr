{
  "compilerOptions": {
    "baseUrl": "app", // DÃ©finit la base des chemins relatifs au dossier app
    "paths": {
      "@/toto/*": [
        "toto/*"
      ], // Alias pour tous les fichiers dans le dossier toto sous app
      "@/utils/*": [
        "utils/*"
      ] // Alias pour tous les fichiers dans le dossier utils sous app
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/components/media-items/media-items-list.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}